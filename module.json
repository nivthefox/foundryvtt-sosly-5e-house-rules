  "authors": [
    {
      "name": "nivthefox",
      "discord": "nivthefox",
      "flags": {}
    }
  ],
  "compatibility": {
    "minimum": "12",
    "verified": "12"
  },
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
